{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "loading-sp-data-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/loadingSpData/LoadingSpDataWebPart.js",
          "manifest": "./src/webparts/loadingSpData/LoadingSpDataWebPart.manifest.json"
        }
      ]
    },
    "mock-data-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/mockData/MockDataWebPart.js",
          "manifest": "./src/webparts/mockData/MockDataWebPart.manifest.json"
        }
      ]
    },
    "create-item-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createItem/CreateItemWebPart.js",
          "manifest": "./src/webparts/createItem/CreateItemWebPart.manifest.json"
        }
      ]
    },
    "o-data-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/oDataWp/ODataWpWebPart.js",
          "manifest": "./src/webparts/oDataWp/ODataWpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "LoadingSpDataWebPartStrings": "lib/webparts/loadingSpData/loc/{locale}.js",
    "MockDataWebPartStrings": "lib/webparts/mockData/loc/{locale}.js",
    "CreateItemWebPartStrings": "lib/webparts/createItem/loc/{locale}.js",
    "ODataWpWebPartStrings": "lib/webparts/oDataWp/loc/{locale}.js"
  }
}
