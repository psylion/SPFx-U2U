{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "libraries-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/libraries/LibrariesWebPart.js",
          "manifest": "./src/webparts/libraries/LibrariesWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "globalName": "jquery",
      "path": "https://code.jquery.com/jquery-3.3.1.min.js"
    },
    "cycle": {
      "globalName": "jquery",
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery.cycle/3.0.3/jquery.cycle.all.min.js",
      "globalDependencies": ["jquery"]
    },
    "angular": {
      "globalName": "angular",
      "path": "https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.7.5/angular.min.js"
    }
  },
  "localizedResources": {
    "LibrariesWebPartStrings": "lib/webparts/libraries/loc/{locale}.js"
  }
}
